{
    "name": "astrotomic/php-conditional-proxy",
    "description": "This package provides a trait and class to allow calling methods based on a condition without breaking the method chain.",
    "keywords": [
        "conditional-proxy",
        "proxy"
    ],
    "homepage": "https://github.com/Astrotomic/php-conditional-proxy",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://astrotomic.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^0.2.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\ConditionalProxy\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest"
    }
}
